{
  "name": "grainjs",
  "version": "0.0.1",
  "description": "JS library from Grist Labs",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha -R list --harmony test",
    "build": "mkdir -p dist && browserify index.js -o dist/grain-only.debug.js --no-bundle-external && babili dist/grain-only.debug.js -o dist/grain-only.min.js",
    "buildall": "mkdir -p dist && browserify index.js -o dist/grain-full.debug.js && babili dist/grain-full.debug.js -o dist/grain-full.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gristlabs/grainjs.git"
  },
  "keywords": [
    "grist",
    "gristlabs",
    "javascript"
  ],
  "author": "Grist Labs",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gristlabs/grainjs/issues"
  },
  "homepage": "https://github.com/gristlabs/grainjs#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babili": "^0.1.4",
    "browserify": "^14.4.0",
    "chai": "^4.1.0",
    "jsdom": "^9.4.1",
    "knockout": "^3.4.2",
    "mocha": "^3.0.1",
    "sinon": "^2.4.1"
  },
  "dependencies": {
    "fast.js": "^0.1.1",
    "fastpriorityqueue": "^0.2.3",
    "lodash": "^4.15.0"
  }
}
